<tr id="<%= dom_id(article) %>">
  <td><%= article.title %></td>
  <td><%= article.content %></td>
  <td><%= article.author %></td>
  <td><%= article.date %></td>
  <td>
    <turbo-frame id="<%= dom_id(article) %>">
      <%= link_to 'Edit', edit_article_path(article), data: { turbo_frame: dom_id(article) } %>
      <%= link_to 'Destroy', article, method: :delete, data: { turbo_method: :delete, turbo_confirm: 'Are you sure?' } %>
    </turbo-frame>
  </td>
</tr>
