<h1>
    Encyclopedia
</h1>

<h3>List of Articles</h3>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Content</th>
      <th>Author</th>
      <th>Date</th>
    </tr>
  </thead>
  <tbody>
    <% @articles.each do |article| %>
      <tr>
        <td><%= article.title %></td>
        <td><%= article.content %></td>
        <td><%= article.author %></td>
        <td><%= article.date %></td>
      </tr>
    <% end %>
  </tbody>
</table>
