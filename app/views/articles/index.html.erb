<h1>Articles</h1>
<p><i>There are <%=Article.count%></i></p>

<%= link_to "<button>Create a new entry</button>".html_safe, new_article_path%>

<%# Creates a list of article titles that contain hyper links to view the full article %>
<ul>
  <% @articles.each do |article| %>
    <li>
        <%= link_to article.title, article %>
    </li>
  <% end %>
</ul>
